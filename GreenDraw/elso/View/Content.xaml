                <MenuItem Header="_Kilépés" Click="ckilep" Name="kilep"/>
            </MenuItem>
            <MenuItem Header="_Böngészés">
                <MenuItem Header="_Kép keresése" Click="ckepkeres" Name="kepkeres"/>
            </MenuItem>
            <MenuItem Header="_Felhasználó">
                <MenuItem Header="_Bejelentkezés" Click="cbejelentkez" Name="bejelentkez"/>
                <MenuItem Header="_Regisztráció" Click="cregisztal" Name="regisztal"/>
                <MenuItem Header="_Fiók beállítások" Click="cfiokbeall" Name="fiokbeall"/>
                <MenuItem Header="_Kijelentkezés" Click="ckijelent" Name="kijelent"/>
                <Separator />
                <MenuItem Header="_Felhasználók keresése" Click="cfelhasznaloker" Name="felhasznaloker"/>
                <MenuItem Header="_Elfogadásraváró képek" Click="celfogadasravarokepek" Name="elfogadasravarokepek"/>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
