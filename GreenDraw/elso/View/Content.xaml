        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="win4" Height="477" Width="830" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button Content="vissza" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click" Margin="747,0,0,0"/>
