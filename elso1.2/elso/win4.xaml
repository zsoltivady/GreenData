                <MenuItem Header="_Kilépés"/>
            </MenuItem>
            <MenuItem Header="_Böngészés">
                <MenuItem Header="_Kép keresése"/>
            </MenuItem>
            <MenuItem Header="_Felhasználó">
                <MenuItem Header="_Bejelentkezés"/>
                <MenuItem Header="_Regisztráció"/>
                <MenuItem Header="_Fiók beállítások"/>
                <Separator />
                <MenuItem Header="_Felhasználók keresése"/>
                <MenuItem Header="_Elfogadásraváró képek"/>
            </MenuItem>
        </Menu>
    </Grid>
