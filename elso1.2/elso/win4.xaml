        <Label Content="Keresés" HorizontalAlignment="Left" Margin="284,0,0,0" VerticalAlignment="Top"/>
        <Menu HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="179" RenderTransformOrigin="0.5,0.5" Margin="2,0,0,0">
            <Menu.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
